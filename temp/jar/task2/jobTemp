#!/bin/bash
# Job name:
#SBATCH --job-name=jobTemp
#
#Project:
#SBATCH --account=uio
#
#Wall clock limit
#SBATCH --time=0:10:0
#
#Max memory usage:
#SBATCH --mem-per-cpu=700M

source /cluster/bin/jobsetup
module purge  # clear any inherited modules

## Copy input files to the work directory
cp -R /usit/abel/u1/dipeshpr/App/task2 $SCRATCH

# DO some work
cd $SCRATCH
cd task2
chmod +x jar1.jar
java -jar jar1.jar 1
sleep 8
cp result.txt /usit/abel/u1/dipeshpr/AppResult/task2
