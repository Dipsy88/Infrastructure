#!/bin/bash
# Job name:
#SBATCH --job-name=jobTemp
#
#Project:
#SBATCH --account=uio
#
#Wall clock limit
#SBATCH --time=00:05:00
#
#Max memory usage:
#SBATCH --mem-per-cpu=700M

source /cluster/bin/jobsetup
module purge  # clear any inherited modules
module load python2/2.7.9
pip install --user pexpect

## Copy input files to the work directory
cp -R /usit/abel/u1/dipeshpr/App/task47 $SCRATCH

# DO some work
cd $SCRATCH
cd task47
chmod +x jar1.jar
java -jar jar1.jar 'TestCases/src/Main/__init__.py' '1'
sleep 8
mv result.txt task47.txt
cp task47.txt /usit/abel/u1/dipeshpr/AppResult
